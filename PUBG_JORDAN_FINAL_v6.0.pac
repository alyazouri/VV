function isJordanDomain(host) {
  var h = host.toLowerCase();
  if (shExpMatch(h, "*.jo") || shExpMatch(h, "*.orange.jo") || 
      shExpMatch(h, "*.zain.jo") || shExpMatch(h, "*.umniah.jo") ||
      shExpMatch(h, "*.umniah.com")) return true;
  if (/jordan|amman|zarqa|irbid|aqaba/.test(h)) return true;
  return false;
}

function isBlockedDomain(host) {
  var h = host.toLowerCase();
  if (shExpMatch(h, "*.eg") || /egypt|cairo/.test(h)) return true;
  if (shExpMatch(h, "*.ir") || /iran|tehran/.test(h)) return true;
  if (shExpMatch(h, "*.af") || /afghan|kabul/.test(h)) return true;
  return false;
}

/* ==============================
   üéÆ ŸÉÿ¥ŸÅ PUBG
   ============================== */
function isPUBG(host) {
  var h = host.toLowerCase();
  if (/pubg|pubgm|krafton|proximabeta|lightspeed|levelinfinite|battlegrounds|playfab/.test(h)) return true;
  if (/tencent|qcloud|myqcloud|tencentcs|tdm/.test(h)) return true;
  return false;
}

function isMatch(host) {
  var h = host.toLowerCase();
  return /match|battle|game|combat|realtime|session|room|playfab|allocation|dispatcher/.test(h);
}

function isLobby(host) {
  var h = host.toLowerCase();
  return /lobby|matchmaking|queue|dispatch|gateway|region|join|recruit|presence/.test(h);
}

function isCDN(host) {
  var h = host.toLowerCase();
  return /cdn|asset|patch|update|download|static|resource|media|content/.test(h);
}

function isAuth(host) {
  var h = host.toLowerCase();
  return /auth|login|token|oauth|account|user|profile/.test(h);
}

function isSocial(host) {
  var h = host.toLowerCase();
  return /friend|invite|squad|team|party|clan|chat|message|social/.test(h);
}

/* ==============================
   üì¶ DNS Cache
   ============================== */
var DNS_CACHE = {};

function resolveIP(host) {
  if (DNS_CACHE[host]) return DNS_CACHE[host];
  try {
    var ip = dnsResolve(host);
    if (ip) {
      DNS_CACHE[host] = ip;
      return ip;
    }
  } catch(e) {}
  return null;
}

/* ==============================
   üöÄ ÿßŸÑŸÖÿ≠ÿ±ŸÉ ÿßŸÑÿ±ÿ¶Ÿäÿ≥Ÿä
   ============================== */
function FindProxyForURL(url, host) {
  var h = host.toLowerCase();

  // === ŸÖŸàÿßŸÇÿπ ŸÖÿ®ÿßÿ¥ÿ±ÿ© ===
  if (dnsDomainIs(h, "github.com") || shExpMatch(h, "*.github.com") ||
      dnsDomainIs(h, "githubusercontent.com") || shExpMatch(h, "*.githubusercontent.com") ||
      dnsDomainIs(h, "githubassets.com") || shExpMatch(h, "*.githubassets.com") ||
      dnsDomainIs(h, "youtube.com") || shExpMatch(h, "*.youtube.com") ||
      shExpMatch(h, "*.googlevideo.com") || dnsDomainIs(h, "google.com") ||
      shExpMatch(h, "*.google.com")) {
    return DIRECT;
  }

  // === ÿ≠ÿ∏ÿ± ÿßŸÑÿØŸàŸÖŸäŸÜÿßÿ™ ÿßŸÑŸÖÿ≠ÿ¨Ÿàÿ®ÿ© ===
  if (isBlockedDomain(h)) return BLOCK;

  // === ŸÅÿ≠ÿµ PUBG ===
  if (isPUBG(h)) {
    var ip = resolveIP(h);
    
    if (!ip || ip.indexOf(":") > -1) return BLOCK;
    if (isBlockedIP(ip)) return BLOCK;

    // === MATCH: ÿ™ÿ´ÿ®Ÿäÿ™ ÿßŸÑÿ®ÿ±ŸàŸÉÿ≥Ÿä ===
    if (isMatch(h)) {
      // üí° ÿßŸÑŸÅŸÉÿ±ÿ©: ÿßŸÑÿ≥ŸÖÿßÿ≠ ŸÑŸÑÿ£ÿ±ÿØŸÜ + ÿßŸÑÿØŸàŸÑ ÿßŸÑÿµÿØŸäŸÇÿ©
      if (!isJordanIP(ip) && !isFriendlyIP(ip) && !isJordanDomain(h)) {
        return BLOCK;
      }
      
      var net24 = ip.split('.').slice(0, 3).join('.');
      
      if (!SESSION.matchNet) {
        SESSION.matchNet = net24;
        SESSION.matchHost = h;
        return PROXY_MATCH;
      }
      
      if (h === SESSION.matchHost || net24 === SESSION.matchNet) {
        return PROXY_MATCH;
      }
      
      SESSION.matchNet = net24;
      SESSION.matchHost = h;
      return PROXY_MATCH;
    }

    // === LOBBY: ŸÜŸÅÿ≥ ÿßŸÑÿ®ÿ±ŸàŸÉÿ≥Ÿä ÿØÿßÿ¶ŸÖÿßŸã ===
    if (isLobby(h) || isSocial(h) || isAuth(h)) {
      // üí° ÿßŸÑŸÅŸÉÿ±ÿ©: ÿßŸÑÿ≥ŸÖÿßÿ≠ ŸÑŸÑÿ£ÿ±ÿØŸÜ + ÿßŸÑÿØŸàŸÑ ÿßŸÑÿµÿØŸäŸÇÿ©
      if (isJordanIP(ip) || isFriendlyIP(ip) || isJordanDomain(h)) {
        return getLobbyProxy();
      }
      return BLOCK;
    }

    // === CDN: ŸÖÿ®ÿßÿ¥ÿ± ŸÑŸÑÿ≥ÿ±ÿπÿ© ===
    if (isCDN(h)) return DIRECT;

    // === ÿ®ÿßŸÇŸä PUBG ===
    if (isJordanIP(ip) || isFriendlyIP(ip) || isJordanDomain(h)) {
      return getLobbyProxy();
    }
    
    // üí° ÿßŸÑŸÅŸÉÿ±ÿ©: ÿßŸÑÿ≥ŸÖÿßÿ≠ ÿ®ÿ®ÿßŸÇŸä ÿßŸÑÿÆŸàÿßÿØŸÖ
    return getLobbyProxy();
  }

  // === ÿ®ÿßŸÇŸä ÿßŸÑŸÖŸàÿßŸÇÿπ ===
  if (isJordanDomain(h)) return DIRECT;
  
  var ip = resolveIP(h);
  if (ip) {
    if (isJordanIP(ip) || isFriendlyIP(ip)) return DIRECT;
    if (isBlockedIP(ip)) return BLOCK;
  }

  return DIRECT;
}
